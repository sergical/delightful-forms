{
  "name": "delightful-forms",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "build:slides": "turbo build --filter=slides",
    "build:demo": "turbo build --filter=demo"
  },
  "devDependencies": {
    "prettier": "^3.5.2",
    "turbo": "^2.4.4",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.5.1",
  "engines": {
    "node": ">=18"
  }
}
